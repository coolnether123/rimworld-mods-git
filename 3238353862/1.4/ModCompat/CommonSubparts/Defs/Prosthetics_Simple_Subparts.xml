<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<!-- Prosthetic hand -->

	<HediffDef ParentName="AddedSimpleBodyPartAnimalBase">
		<defName>SimpleProstheticHandAnimal</defName>
		<label>prosthetic animal hand</label>
		<labelNoun>a prosthetic animal hand</labelNoun>
		<description>An installed prosthetic animal hand. While it lacks a neural interface, its complex arrangement of internal joints allows it to mimic natural movement quite convincingly. Still, it is inferior to a real animal's hand.</description>
		<descriptionHyperlinks><ThingDef>SimpleProstheticHandAnimal</ThingDef></descriptionHyperlinks>
		<spawnThingOnRemoved>SimpleProstheticHandAnimal</spawnThingOnRemoved>
		<addedPartProps>
			<solid>true</solid>
			<partEfficiency>0.8</partEfficiency>
		</addedPartProps>
		<comps>
			<li Class="HediffCompProperties_VerbGiver">
				<tools>
					<li>
						<label>fist</label>
						<capacities>
							<li>Blunt</li>
						</capacities>
						<power>3.6</power>
						<cooldownTime>1.5</cooldownTime>
					</li>
				</tools>
			</li>
		</comps>
	</HediffDef>
  
	<ThingDef ParentName="BodyPartProstheticAnimalBase">
		<defName>SimpleProstheticHandAnimal</defName>
		<label>prosthetic animal hand</label>
		<description>An animal hand prosthesis. While it lacks a neural interface, its complex arrangement of internal joints allows it to mimic natural movement quite convincingly. Still, it is inferior to a real animal's hand. Fits all animals.</description>
		<descriptionHyperlinks><RecipeDef>InstallSimpleProstheticHandAnimal</RecipeDef></descriptionHyperlinks>
		<costList>
			<Steel>15</Steel>
			<ComponentIndustrial>2</ComponentIndustrial>
		</costList>
		<statBases>
			<Mass>2</Mass>
		</statBases>
	</ThingDef>

	<RecipeDef ParentName="SurgeryInstallSimpleProstheticBodyPartAnimalBase">
		<defName>InstallSimpleProstheticHandAnimal</defName>
		<label>install prosthetic animal hand</label>
		<description>Installs a prosthetic animal hand.</description>
		<descriptionHyperlinks>
			<ThingDef>SimpleProstheticHandAnimal</ThingDef>
			<HediffDef>SimpleProstheticHandAnimal</HediffDef>
		</descriptionHyperlinks>
		<jobString>Installing prosthetic animal hand.</jobString>
		<addsHediff>SimpleProstheticHandAnimal</addsHediff>
		<appliedOnFixedBodyParts>
			<li>Hand</li>
		</appliedOnFixedBodyParts>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>SimpleProstheticHandAnimal</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>SimpleProstheticHandAnimal</li>
			</thingDefs>
		</fixedIngredientFilter>
		<uiIconThing>SimpleProstheticHandAnimal</uiIconThing>
		<recipeUsers>
		</recipeUsers>
	</RecipeDef>


