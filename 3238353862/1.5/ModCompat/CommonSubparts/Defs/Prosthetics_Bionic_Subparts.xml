<?xml version="1.0" encoding="UTF-8"?>
<Defs>

	<!-- Bionic paw -->

	<HediffDef ParentName="AddedBionicBodyPartAnimalBase">
		<defName>BionicPawAnimal</defName>
		<label>bionic animal paw</label>
		<labelNoun>a bionic animal paw</labelNoun>
		<description>An installed bionic animal paw.</description>
		<descriptionHyperlinks><ThingDef>BionicPawAnimal</ThingDef></descriptionHyperlinks>
		<spawnThingOnRemoved>BionicPawAnimal</spawnThingOnRemoved>
		<addedPartProps>
			<solid>true</solid>
			<partEfficiency>1.2</partEfficiency>
		</addedPartProps>
		<comps>
			<li Class="HediffCompProperties_VerbGiver">
				<tools>
					<li>
						<label>bionic animal paw</label>
						<capacities>
							<li>Scratch</li>
						</capacities>
						<power>18</power>
						<cooldownTime>1.9</cooldownTime>
					</li>
				</tools>
			</li>
		</comps>
	</HediffDef>

	<ThingDef ParentName="BodyPartBionicAnimalBase">
		<defName>BionicPawAnimal</defName>
		<label>bionic animal paw</label>
		<description>An advanced artificial animal paw. With its biogel nerve-link, powerful mini-servos, and lattice-dust healing system, it is better than a biological paw in almost every way. Fits all animals.</description>
		<descriptionHyperlinks><RecipeDef>InstallBionicPawAnimal</RecipeDef></descriptionHyperlinks>
		<costList>
			<Plasteel>7</Plasteel>
			<ComponentSpacer>2</ComponentSpacer>
		</costList>
		<statBases>
			<Mass>3</Mass>
		</statBases>
	</ThingDef>

	<RecipeDef ParentName="SurgeryInstallBionicBodyPartAnimalBase">
		<defName>InstallBionicPawAnimal</defName>
		<label>install bionic animal paw</label>
		<description>Installs bionic paw.</description>
		<descriptionHyperlinks>
			<ThingDef>BionicPawAnimal</ThingDef>
			<HediffDef>BionicPawAnimal</HediffDef>
		</descriptionHyperlinks>
		<jobString>Installing bionic animal paw.</jobString>
		<addsHediff>BionicPawAnimal</addsHediff>
		<appliedOnFixedBodyParts>
			<li>Paw</li>
		</appliedOnFixedBodyParts>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>BionicPawAnimal</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>BionicPawAnimal</li>
			</thingDefs>
		</fixedIngredientFilter>
		<uiIconThing>BionicPawAnimal</uiIconThing>
		<recipeUsers>
		</recipeUsers>
	</RecipeDef>


