	</HediffDef>
	

	<RecipeDef Name="SurgeryInstallMedievalBodyPartAnimalBase" ParentName="SurgeryInstallArtificialBodyPartAnimalBase" Abstract="True">
		<skillRequirements>
			<Medicine>3</Medicine>
		</skillRequirements>
		<workAmount>1500</workAmount>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>WoodLog</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<descriptionHyperlinks><ThingDef>WoodLog</ThingDef></descriptionHyperlinks>
		<fixedIngredientFilter>
			<thingDefs>
