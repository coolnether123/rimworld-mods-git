<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <TerrainDef Name="FloorPlasteel">
    <defName>FloorPlasteel</defName>
    <label>plasteel floor</label>
    <description>This high-tech floor never gets dirty and so is suitable for use in sterile environments. It's also pleasing to look at (in a slick, space-age sort of way). Expensive and takes a considerable time to build.</description>
    <texturePath>Terrain/Surfaces/PlasteelFloor</texturePath>
    <layerable>true</layerable>
    <affordances>
      <li>Light</li>
      <li>Medium</li>
      <li>Heavy</li>
    </affordances>
    <tags>
      <li>Floor</li>
    </tags>
    <statBases>
      <Cleanliness>0.6</Cleanliness>
      <WorkToBuild>1200</WorkToBuild>
      <Beauty>3</Beauty>
    </statBases>
    <costList>
      <Plasteel>8</Plasteel>
      <ComponentIndustrial>1</ComponentIndustrial>
    </costList>
    <designationCategory>MoreFloors</designationCategory>
    <fertility>0</fertility>
    <constructEffect>ConstructMetal</constructEffect>
    <filthAcceptanceMask></filthAcceptanceMask>
    <terrainAffordanceNeeded>Heavy</terrainAffordanceNeeded>
    <researchPrerequisites>
      <li>SterileMaterials</li>
    </researchPrerequisites>
    <pollutionShaderType MayRequire="Ludeon.RimWorld.Biotech">TerrainFadeRoughLinearBurn</pollutionShaderType>
    <pollutionOverlayTexturePath>Terrain/Surfaces/PollutionFloorSmooth</pollutionOverlayTexturePath>
    <pollutionColor>(1, 1, 1, 0.8)</pollutionColor>
    <pollutionOverlayScale>(0.75, 0.75)</pollutionOverlayScale>
  </TerrainDef>

</Defs>
