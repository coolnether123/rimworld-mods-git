<?xml version="1.0" encoding="UTF-8"?>
<Patch>

   <!-- ========== Archotech Base ========== -->
   
	<Operation Class="PatchOperationReplace">
		<xpath>Defs/ThingDef[@Name="BodyPartArchotechBase"]/graphicData</xpath>
		<value>
			<graphicData>
				<texPath>Things/Item/BodyPart/ArchotechPart</texPath>
				<graphicClass>Graphic_Single</graphicClass>
				<drawSize>0.80</drawSize>
			</graphicData>
		</value>
	</Operation>

   <!-- ========== Archotech Eye ========== -->
   
	<Operation Class="PatchOperationAdd">
		<xpath>Defs/HediffDef[defName="ArchotechEye"]</xpath>
		<value>
			<defaultLabelColor>(225,237,114)</defaultLabelColor>
		</value>
	</Operation>

   <!-- ========== Archotech Eye ========== -->
   
	<Operation Class="PatchOperationAdd">
