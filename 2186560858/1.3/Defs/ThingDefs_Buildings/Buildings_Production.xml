<?xml version="1.0" encoding="utf-8" ?>
<Defs>



  <ThingDef ParentName="BenchBase">
    <defName>VBE_Bar</defName>
    <label>bar</label>
    <description>A somewhat ornate countertop stocked to the brim with different vessels and devices for making a variety of drinks. An ice box, a series of taps, hoses, and carbonators means anybody tending it can brew much quicker than at a rudimentary brewery.</description>
    <thingClass>Building_WorkTable</thingClass>
    <graphicData>
      <texPath>Things/Buildings/VBE_TableBar</texPath>
      <graphicClass>Graphic_Multi</graphicClass>
      <drawSize>(3.5,1.5)</drawSize>
	  <shaderType>CutoutComplex</shaderType>
      <shadowData>
        <volume>(2.8, 0.3, 0.54)</volume>
        <offset>(0, 0, -0.18)</offset>
      </shadowData>
      <damageData>
        <rect>(0.3,0,2.7,1)</rect>
        <cornerTR>Damage/Corner</cornerTR>
        <cornerBR>Damage/Corner</cornerBR>
      </damageData>
    </graphicData>
  
	 <costList>
      <Steel>30</Steel>
      <ComponentIndustrial>2</ComponentIndustrial>
    </costList>
    <stuffCategories>
      <li>Metallic</li>
      <li>Woody</li>
    </stuffCategories>
    <costStuffCount>150</costStuffCount>
		
    <altitudeLayer>Building</altitudeLayer>
    <fillPercent>0.5</fillPercent>
    <useHitPoints>True</useHitPoints>
    <statBases>
      <WorkToBuild>2500</WorkToBuild>
      <MaxHitPoints>200</MaxHitPoints>
      <Flammability>1.0</Flammability>
    </statBases>
    <size>(3,1)</size>
    <designationCategory>Production</designationCategory>
    <passability>PassThroughOnly</passability>
    <pathCost>70</pathCost>
    <hasInteractionCell>True</hasInteractionCell>
    <interactionCellOffset>(0,0,-1)</interactionCellOffset>
    <surfaceType>Item</surfaceType>
    
    <inspectorTabs>
      <li>ITab_Bills</li>
    </inspectorTabs>
    <building>
      <spawnedConceptLearnOpportunity>BillsTab</spawnedConceptLearnOpportunity>
    </building>
    <placeWorkers>
      <li>PlaceWorker_PreventInteractionSpotOverlap</li>
    </placeWorkers>
    <researchPrerequisites>
      <li>VBE_MixologyResearch</li>
    </researchPrerequisites>
  </ThingDef>

  

</Defs>
